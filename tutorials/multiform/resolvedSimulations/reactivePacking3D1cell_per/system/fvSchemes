FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

wallDist
{
        method Poisson;

        // Optional entry enabling the calculation
        // of the normal-to-wall field
        nRequired false;
}

ddtSchemes
{
    //default         steadyState;
    ddt(T)           backward;
    ddt(U)           steadyState;
    dynamicRetention steadyState;
}

gradSchemes
{
    default         cellLimited Gauss linear 1;// pointCellsLeastSquares;
}

divSchemes
{
    default         none;
    div(phi,U)      Gauss linearUpwind grad(U);
    div(phi,T)      bounded Gauss linearUpwind grad(T);
    div(phip,T)      bounded Gauss linearUpwind grad(T);
    div((nuEff*dev2(T(grad(U))))) Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


// ************************************************************************* //
