FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
		value 			uniform 1;
    }

    "solid.*"
    {
        type            zeroGradient;
    }

    outlet
    {
		type            zeroGradient;
		value 			uniform 0;    }

    "symm.*"
    {
        type            symmetryPlane;
    }

    frontAndBack
    {
        type            symmetry;
    }
}

