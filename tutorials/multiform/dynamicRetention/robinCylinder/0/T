FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 1;
    }

    cylinder
    {
        type            Robin;
        value           uniform 0;
        RobinD          uniform 1.0;
        RobinK          uniform -0.01;
        RobinF          uniform 0;

    }

    up
    {
        type            symmetryPlane;
    }

    down
    {
        type            symmetryPlane;
    }

    frontAndBack
    {
        type            empty;
    }

    right
    {
        type            zeroGradient;
    }
}

