FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "mu.*|alpha.*"
    {
        solver          PBiCGStab;
        preconditioner  diagonal;
        tolerance       1e-8;
        relTol          0;
    }
}

PIMPLE
{
    debugCH false;
    lambdaMu 1e6;
    lambdaAlpha 1e6;
    muRelax 1;
    alphaRelax 1;
    muRelaxTime 0.9;
    alphaRelaxTime 0.9;
    nNonOrthogonalCorrectors 0;
    nOuterCorrectors 300;
    
    outerCorrectorResidualControl
    {
        alpha
        {
            tolerance 1e-5;
            relTol    1e-4;
        }

        mu
        {
            tolerance 1e-5;
            relTol    1e-4;
        }

    }
}

relaxationFactors
{
    equations
    {
       alpha    1;
       mu       1;
    }

    fields
    {
        alpha   1;
        mu      1;
    }

}

