FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 0 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{
    "solid.*"
    {
        type            Robin;
        RobinD          uniform 0.1;
        RobinK          uniform -1;
        RobinF          uniform 0.0;
        value           uniform 0.0;

    }
    frontAndBack
    {
        type            empty;
    }
    ".*.cyc"
    {
        type            cyclic;
    }

}


