FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      source;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            cyclic;
        value           uniform 0;
    }

    outlet
    {
        type            cyclic;
        value           uniform 0;
    }

    up
    {
        type            cyclic;
        value           uniform 0;
    }

    down
    {
        type            cyclic;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
        value           uniform 0;
    }
}

