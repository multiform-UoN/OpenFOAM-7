FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
  inlet
  {
      type            zeroGradient;//fixedJump;
      //patchType       cyclic;
      //jump            uniform -10;
      //value           uniform 100000;
    }

    side
    {
        type            fixedValue;//fixedJump;
        //patchType       cyclic;
        //jump            uniform -10;
        value           uniform 0;
    }

    outlet
    {
        type            zeroGradient;//fixedJump;
        //patchType       cyclic;
        //jump            uniform 10;
        //value           uniform 100000;
    }

   top
    {
        type            zeroGradient;
        value           cyclic;
    }

	topmid
    {
        type            fixedFluxPressure;//fixedValue;
        value           uniform 100;
    }	

    bottom
    {
        type            fixedFluxPressure;//zeroGradient;
        //value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}

