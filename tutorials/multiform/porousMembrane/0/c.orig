FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      c;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
  inlet
  {
        type            zeroGradient;
    }

    side
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    top
    {
        type            zeroGradient;
       // value           uniform 1;
    }

    topmid
    {
        type            fixedValue;
        value           uniform 1;
    }

    bottom
    {
        type            dynamicRetention;
        RobinD          uniform 1e-5;  //PUT THE DIFFUSION COEFFICIENT HERE
        RobinK          uniform -1;

        value           uniform 0;
        S               uniform 0;
        Kd              uniform 0;
        forwardReaction
        {
          type          constant;
        }
        backwardReaction
        {
          type          constant;
        }
    }

    frontAndBack
    {
        type            empty;
    }
}

