FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      c;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
  inlet_additive
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
        value           uniform 0;
    }

    inlet_oil
    {
        type            fixedValue;
        value		    uniform 1;
    }

    warm_walls
    {
        type            zeroGradient;
    }

    cold_walls
    {
        type            zeroGradient;
    }
    empty
    {
        type            empty;
    }
}

