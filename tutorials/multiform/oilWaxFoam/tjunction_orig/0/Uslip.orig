FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      Uslip;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet_additive
    {
           type            zeroGradient; 
    }

    outlet
    {
        type            noSlip;
    }

    inlet_oil
    {
          type            zeroGradient; //fixedValue;
        value           uniform (0.1 0 0);
    }

    warm_walls
    {
       type            noSlip;
    }

    cold_walls
    {
        type            noSlip;
    }
    empty
    {
        type            empty;
    }
}

