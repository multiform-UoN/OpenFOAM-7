FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wall-4
    {
        type            noSlip;
    }
    
    velocity-inlet-5
    {
        type            zeroGradient; //fixedValue;
        value           uniform (0.1 0 0);
    }
    velocity-inlet-6
    {
        type            noSlip;
    }
    pressure-outlet-7
    {
        type            zeroGradient;
    }
    wall-8
    {
        type            noSlip;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}

