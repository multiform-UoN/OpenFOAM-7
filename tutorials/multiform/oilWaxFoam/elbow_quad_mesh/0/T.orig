FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1;

boundaryField
{
   wall-4
    {
        type            fixedValue;
        value           uniform 0;
    }
    
    velocity-inlet-5
    {
        type            fixedValue;
        value           uniform 1;
    }
    velocity-inlet-6
    {
       type            fixedValue;
        value           uniform 0;
    }
    pressure-outlet-7
    {
       type            zeroGradient;
    }
    wall-8
    {
       type            fixedValue;
        value           uniform 0;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}

