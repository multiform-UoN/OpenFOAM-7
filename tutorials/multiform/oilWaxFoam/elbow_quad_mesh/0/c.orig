FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      c;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
   wall-4
    {
        type            zeroGradient;
    }
    
    velocity-inlet-5
    {
        type            fixedValue;
        value		    uniform 1;
    }
    velocity-inlet-6
    {
        type            zeroGradient;
    }
    pressure-outlet-7
    {
        type            zeroGradient;
        value           uniform 0;
    }
    wall-8
    {
        type            zeroGradient;
    }

    frontAndBackPlanes
    {
        type            empty;
    }

}

