DT              DT   [0 2 -1 0 0 0 0]    1e-6;
psim            psim [0 0 0 0 0 0 0]      1.2;
St              St   [0 0 1 0 0 0 0]    1e-12;
tau1            tau1 [0 0 1 0 0 0 0]     1e-3;
tau2            tau2 [0 0 1 0 0 0 0]     1e12;
crif            crif [0 -3 0 0 0 0 0]       1;
cT              crif [0 -3 0 -1 0 0 0]      1;
//gamma           gamma[0 2 -1 0 0 0 0] 2.76e-9;
dh              dh   [1 2 -2 0 0 0 0]       1;
ds              ds   [1 2 -2 -1 0 0 0]      1;
