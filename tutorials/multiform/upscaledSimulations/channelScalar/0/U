FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "288";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

upvel #calc "0.06630084743799461/0.7";

internalField  uniform ($upvel 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform ($upvel 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    upAndDown
    {
        type            empty;
    }
    frontAndBack
    {
        type            empty;
    }
}


