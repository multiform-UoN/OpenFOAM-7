FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
    grad(U)         Gauss linear;
}

divSchemes
{
	default         none;
    div(phi,C)      Gauss upwind;
    div(phiNP,C)    Gauss upwind;
}

laplacianSchemes
{
    default                  Gauss linear corrected;
}

interpolationSchemes
{
    default           linear;

}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p;
}

