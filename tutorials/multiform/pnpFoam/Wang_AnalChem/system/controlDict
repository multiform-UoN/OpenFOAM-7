FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
libs            ("libelectrokineticFoam.so");

application     pnpFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1e-2;

deltaT          1e-7;

writeControl    runTime;

writeInterval   1e-3;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable false;

adjustTimeStep false;

maxCo          1.0;

maxDeltaT      1e-3;

rDeltaTSmoothingCoeff 0.05;

rDeltaTDampingCoeff 0.8;

functions
{
  
  #includeFunc singleGraph
  #includeFunc WallGrad
  #includeFunc residuals
  /*
  #includeFunc Inner_E_Field_Slice
  */
  #includeFunc Surf_Concen_Average

}


