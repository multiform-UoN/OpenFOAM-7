FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Inner_Pip_Wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    Outer_Pip_Wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    Inner_Electrode
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    Planar_Electrode
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    Outer_Walls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

#includeEtc "caseDicts/setConstraintTypes"

}

