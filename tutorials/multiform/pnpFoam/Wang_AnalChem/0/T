FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298.13;

boundaryField
{
    Inner_Pip_Wall
    {
        type            zeroGradient;
    }

    Outer_Pip_Wall
    {
        type            zeroGradient;
    }

    Inner_Electrode
    {
        type            zeroGradient;
    }

    Planar_Electrode
    {
        type            zeroGradient;
    }

    Outer_Walls
    {
        type            zeroGradient;
    }

#includeEtc "caseDicts/setConstraintTypes"

}

