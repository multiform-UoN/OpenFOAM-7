FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    V
    {
        solver          PBiCGStab;
        preconditioner  DIC;
        smoother  FDIC;
        tolerance       1e-12;
        relTol          0;
    }

    VFinal
    {
        $V;
        relTol          0;
    }

    "C.*"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        smoother         DILU;
        tolerance       1e-12;
        relTol          0;
    }
}

couplingControls
{
    nCorrectors    5;
    nNonOrthogonalCorrectors 0;
    //VRefCell        0;
    //VRefValue       0;

    residualControl
    {
        ".*"            1e-5;
    }
}

relaxationFactors
{
	fields
	{
		C	1;
	}
    equations
    {
       "C.*"  1;
        V     1;
    }
}



