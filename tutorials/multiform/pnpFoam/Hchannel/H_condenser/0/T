FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
	"BottomLeftLeg|BottomRightLeg"
	{
		type            noFlux;
		value           $internalField;
	}

	"TopLeftLeg|TopRightLeg"
	{
		type		noFlux;
		value		$internalField;
	}

	"FixedWallsTop|FixedWallsBottom"
	{
		type		empty;
	}

	"FixedWallsLeft|FixedWallsRight"
	{
		type		noFlux;
		value		$internalField;
	}
	
	"BottomInnerWall|TopInnerWall"
	{
		type		noFlux;
		value		$internalField;
	}
}

// ************************************************************************* //
