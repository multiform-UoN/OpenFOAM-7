FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      C.anions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 -3 0 0 1 0 0 ];

internalField   uniform 2e-3;

boundaryField
{
    "anode|cathode"
    {
        type            RobinPhi;
        phiName         "phiNP";
        value           uniform 2e-3;
        RobinD          uniform 1e-09;
        RobinK          uniform 0;
        RobinF          uniform 0;
    }

    fixedWalls
    {
        type            noFlux;
        value           $internalField;
    }

    frontAndBack
    {
        type            empty;
    }
}

