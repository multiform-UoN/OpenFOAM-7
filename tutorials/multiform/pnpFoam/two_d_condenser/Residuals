set logscale y
set title "Residuals"
set ylabel 'Residual'
set xlabel 'Iteration'
plot "< cat log | grep 'V' | cut -d' ' -f9 | tr -d ','" title 'V' with lines, \
"< cat log | grep 'C.cations' | cut -d' ' -f9 | tr -d ','" title 'C.cations' with lines, \
"< cat log | grep 'C.anions' | cut -d' ' -f9 | tr -d ','" title 'C.anions' with lines
pause 1
reread


