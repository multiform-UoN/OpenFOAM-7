FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../constant/initialConditions"

dimensions      [0 0 0 0 0 0 0];

//- Set initial condition
// internalField uniform 1;
internalField  $alphaInit;


boundaryField
{
    up
    {
        type            cyclic;
    }
    down
    {
        type            cyclic;
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


