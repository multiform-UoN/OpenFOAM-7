FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    alpha
    {
        solver          PBiCGStab;
        preconditioner  diagonal;
        tolerance       1e-8;
        relTol          1e-3;
    }

    alphaFinal
    {
        $alpha;
        tolerance 1e-12;
        relTol    0;
    }

}

PIMPLE
{

    nNonOrthogonalCorrectors 1;
    nOuterCorrectors 300;

    outerCorrectorResidualControl
    {
        alpha
        {
            tolerance 1e-5;
            relTol    1e-4;
        }

        mu
        {
            tolerance 1e-5;
            relTol    1e-4;
        }

    }
}

relaxationFactors
{
    equations
    {
       alpha    1;
       alphaFinal 1;
       mu       1;
    }

    fields
    {
        alpha   0.9;
        alphaFinal 1;
        mu      1;
        muFinal 1;
    }

}

// ************************************************************************* //
