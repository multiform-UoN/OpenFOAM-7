FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      V;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
        value           uniform 0;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    wall
    {
        type            mappedContinuityFluid; // THIS MIGHT NEED TO CHANGE
        Dsolid          uniform 1;
        RobinF          uniform 0;
        RobinK          uniform 0;
        RobinD          uniform 1;
        value           $internalField;
    }

    externalWall
    {
        type           zeroGradient;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


// ************************************************************************* //
