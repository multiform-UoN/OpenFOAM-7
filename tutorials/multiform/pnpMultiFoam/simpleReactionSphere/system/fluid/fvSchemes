FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         pointCellsLeastSquares;
}

divSchemes
{
    default         none;
    div(phi,C)      bounded Gauss linear;
    div(phiNP,C)    bounded Gauss linear;

}

laplacianSchemes
{
    default         Gauss linear corrected;
    laplacian(epsilon_,V) Gauss linear limited 0.5;

}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         limited 0.5;
}

wallDist
{
    method meshWave;
}

// ************************************************************************* //
