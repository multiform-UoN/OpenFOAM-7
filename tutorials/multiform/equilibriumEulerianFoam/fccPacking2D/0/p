FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    maxY
    {
        type            cyclic;
    }
    minX
    {
        type            cyclic;
    }
    maxX
    {
        type            cyclic;
    }
    minY
    {
        type            cyclic;
    }
    minZ
    {
        type            empty;
    }
    maxZ
    {
        type            empty;
    }
    pores
    {
        type            zeroGradient;
    }
}


