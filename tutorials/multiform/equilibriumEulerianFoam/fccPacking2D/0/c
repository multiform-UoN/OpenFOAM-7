FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
  maxY
  {
      type            cyclic;
  }
  minX
  {
      type            cyclic;
  }
  maxX
  {
      type            cyclic;
  }
  minY
  {
      type            cyclic;
  }
  minZ
  {
      type            empty;
  }
  maxZ
  {
      type            empty;
  }
  pores
  {
      type            zeroGradient;
  }
}

